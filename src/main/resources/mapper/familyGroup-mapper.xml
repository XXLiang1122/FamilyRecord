<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="familyGroup">

        <insert id="insert" parameterType="FamilyGroup" useGeneratedKeys="true" keyProperty="id">
                insert family_group(`name`,`creator`) values(#{name},#{creator});
        </insert>

</mapper>